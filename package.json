{
  "name": "@starleaguecompany/react-design-system",
  "version": "0.0.1",
  "description": "Starleaguecompany React Design System",
  "keywords": [
    "design-system",
    "ui-kit",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/starleaguecompany/package-react-design-system",
  "bugs": {
    "url": "https://github.com/starleaguecompany/package-react-design-system/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:starleaguecompany/package-react-design-system.git"
  },
  "author": "Sergey Trushin",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "jsnext:main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "lib"
  ],
  "sideEffects": [
    "dist/*"
  ],
  "scripts": {
    "prepublishOnly": "yarn build",
    "test": "yarn jest --passWithNoTests",
    "lint:scripts": "eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "lint:styles": "stylelint 'src/**/*.less' -s less --fix",
    "lint": "yarn lint:styles && yarn lint:scripts",
    "dev": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "prebuild": "rm -rf dist && rm -rf lib",
    "build": "rollup -c && tsc --declaration --emitDeclarationOnly",
    "format:scripts": "prettier --write 'src/**/*.{ts,tsx}'",
    "format:styles": "stylelint 'src/**/*.less' -s less --fix",
    "format": "yarn format:scripts && yarn format:styles",
    "prepare": "husky install",
    "size": "size-limit",
    "clean": "git clean -Xdf"
  },
  "engines": {
    "node": ">= 12",
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "resolutions": {
    "@types/react": "^17.0.8"
  },
  "dependencies": {
    "@babel/runtime": "^7.15.3",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "date-fns": "^2.23.0",
    "framer-motion": "^4.1.17",
    "react-input-mask": "^3.0.0-alpha.2",
    "react-number-format": "^4.8.0",
    "rollup-plugin-postcss": "^4.0.2"
  },
