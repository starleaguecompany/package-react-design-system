    "@starleaguecompany/react-icons": "^0.1.14",
    "classnames": "^2.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/register": "^7.15.3",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@size-limit/preset-big-lib": "^5.0.2",
    "@starleaguecompany/design-system-theme": "^0.0.1",
    "@starleaguecompany/package-react-utils": "^0.0.1",
    "@starleaguecompany/package-types": "^0.0.1",
    "@starleaguecompany/react-icons": "^0.0.1",
    "@starleaguecompany/rollup-plugin-styles": "^0.0.1",
    "@starleaguecompany/utils": "^0.0.1",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addons": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@storybook/theming": "^6.3.12",
    "@testing-library/dom": "^8.3.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
