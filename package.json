    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "2.53.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^1.4.7",
    "rollup-plugin-visualizer": "^5.5.2",
    "size-limit": "^5.0.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-property-sort-order-smacss": "^7.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.2.2",
    "write-json": "^3.0.1"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "not IE 11"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn format:scripts --",
      "yarn lint:scripts --"
    ],
    "*.less": [
      "yarn format:styles --",
      "yarn lint:styles --"
    ]
  }
}
